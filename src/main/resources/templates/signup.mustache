{{>layout/header}}
<h2>회원 가입</h2>
<div>
    <div class="from-group">
        <label for="email">이메일</label>
        <input type="text" class="from-control" id="email" placeholder="이메일">
        <button type="button" class="btn btn-secondary" id="btn-email-check" onclick="duplicateCheck('email')">이메일 중복 확인</button>
    </div>
    <div class="from-group">
        <label for="password">비밀번호</label>
        <input type="text" class="from-control" id="password" placeholder="비밀번호">
    </div>
    <div class="from-group">
        <label for="name">이름</label>
        <input type="text" class="from-control" id="name" placeholder="이름">
        <button type="button" class="btn btn-secondary" id="btn-name-check" onclick="duplicateCheck('name')">이름 중복 확인</button>
    </div>
    <div class="from-group">
        <label for="blog-name">블로그 이름</label>
        <input type="text" class="from-control" id="blog-name" placeholder="블로그 이름">
        <button type="button" class="btn btn-secondary" id="btn-blog-name-check" onclick="duplicateCheck('blogName')">블로그 이름 중복 확인</button>
    </div>
    <button tybpe="button" class="btn btn-primary" id="btn-signup" onclick="signup()">회원 가입</button>
</div>
<div class="form-group">
    <a href="/">메인으로 가기</a>
</div>
<script src="/js/app/signup.js"></script>
<script src="/js/util/util.js"></script>
{{>layout/footer}}